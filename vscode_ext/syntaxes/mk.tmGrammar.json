{
  "scopeName": "source.abc",
  "patterns": [{ "include": "#expression" }],
  "repository": {
    "expression": {
      "patterns": [{ "include": "#kw" }, { "include": "#constructor" }, { "include": "#commentLine"}]
    },
    "kw": {
      "match": "\\b(fresh|in|import|Sigma|Q|q0|T|delta)\\b",
      "name": "keyword.control"
    },
    "constructor": {
      "match": "\\b([A-Z][_0-9A-Za-z]*)\\b",
      "name": "constant.character"
    },
    "commentLine": {
      "match": "(\/\/.*\\n)",
      "name": "comment.line"
    }
  }
}
